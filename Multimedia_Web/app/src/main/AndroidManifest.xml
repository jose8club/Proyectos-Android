<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="jose.multimedia_web" >

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" >
    </uses-permission>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" >
    </uses-permission>
    <uses-permission android:name="android.permission.INTERNET" >
    </uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Crear"
            android:label="@string/title_activity_crear" >
        </activity>
        <activity
            android:name=".Ver"
            android:label="@string/title_activity_ver" >
        </activity>
        <activity
            android:name=".Id"
            android:label="@string/title_activity_id" >
        </activity>
        <activity
            android:name=".Editar"
            android:label="@string/title_activity_editar" >
        </activity>
        <activity
            android:name=".Eliminar"
            android:label="@string/title_activity_eliminar" >
        </activity>
        <activity
            android:name=".IdDos"
            android:label="@string/title_activity_id_dos" >
        </activity>
        <activity
            android:name=".Video"
            android:label="@string/title_activity_video" >
        </activity>
        <activity
            android:name=".EliminarDos"
            android:label="@string/title_activity_eliminar_dos" >
        </activity>
        <activity
            android:name=".ActualizarDos"
            android:label="@string/title_activity_actualizar_dos" >
        </activity>
    </application>

</manifest>
